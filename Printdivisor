class HelloWorld {
    public static void main(String[] args) {
       
        int  a = 15;
        int count = 0 ;
        for(int i = 1 ; i<=a ; i++){
            if(a%i == 0 ){
                System.out.println(i);
                count++;
            }
            // 
        }
        System.out.println(count);
    }
}
